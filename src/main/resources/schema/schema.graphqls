type Query {
    search(input: SearchInput): [Searchable]
    userDomain: UserDomainQuery!
    problemDomain: ProblemDomainQuery!
    solutionDomain: SolutionDomainQuery!
}

type Mutation {
    userDomain: UserDomainMutation!
    problemDomain: ProblemDomainMutation!
    solutionDomain: SolutionDomainMutation!
}

type Subscription {
    problemAdded: Problem!
    solutionAdded: Solution!
    solutionVoteChanged(solutionId: ID!): Solution!
}
